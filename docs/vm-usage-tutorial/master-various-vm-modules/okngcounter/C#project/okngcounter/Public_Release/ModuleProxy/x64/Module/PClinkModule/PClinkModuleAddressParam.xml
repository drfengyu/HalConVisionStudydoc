<?xml version="1.0" encoding="utf-8"?>
<ParamList>	
    <ParamItem>
		<Name>CpuNo</Name>
		<DefaultValue>1</DefaultValue>
		<Description>CPU号 1-4</Description>
	</ParamItem>
	<ParamItem>
		<Name>ProtocolType</Name>
		<DefaultValue>1</DefaultValue>
		<Description>协议类型 RTU  或 ASCII</Description>
	</ParamItem>
	<ParamItem>
		<Name>IntType</Name>
		<DefaultValue>1</DefaultValue>
		<Description>数据类型，整型，从1开始顺序传递，16位、32位</Description>
	</ParamItem>
	<ParamItem>
		<Name>SoftElementType</Name>
		<DefaultValue>1</DefaultValue>
		<Description>软元件类型，整型，从1开始顺序传递，D\M\X\Y</Description>
	</ParamItem>
	<!-- <ParamItem>
		<Name>FloatSequence</Name>
		<DefaultValue>1</DefaultValue>
		<Description>Float发送顺序</Description>
	</ParamItem> -->
	<ParamItem>
		<Name>RegisterAddress</Name>
		<DefaultValue>0</DefaultValue>
		<Description>寄存器地址</Description>
	</ParamItem>
	<ParamItem>
		<Name>RegisterNum</Name>
		<DefaultValue>2</DefaultValue>
		<Description>寄存器个数</Description>
	</ParamItem>
	<ParamItem>
		<Name>PollEnable</Name>
		<DefaultValue>false</DefaultValue>
		<Description>轮询使能，true：打开轮询使能，flase：关闭轮询使能</Description>
	</ParamItem>
	<ParamItem>
		<Name>OverTime</Name>
		<DefaultValue>200</DefaultValue>
		<Description>超时时间</Description>
	</ParamItem>
	<ParamItem>
		<Name>AssociatedAddress</Name>
		<DefaultValue>0</DefaultValue>
		<Description>关联地址</Description>
	</ParamItem>	
	<ParamItem>
		<Name>Upload</Name>
		<DefaultValue>true</DefaultValue>
		<Description>true：打开数据上传，flase：关闭数据上传</Description>
	</ParamItem>
	
	<!-- 地址相关 -->
</ParamList>