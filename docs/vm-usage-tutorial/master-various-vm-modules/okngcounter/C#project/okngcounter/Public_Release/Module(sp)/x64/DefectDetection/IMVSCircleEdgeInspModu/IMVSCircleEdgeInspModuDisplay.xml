            <Feature Name="Width" Mapping="FlawWidth" Value="0" ArrayMatch="true" />
            <Feature Name="Height" Mapping="FlawHeight" Value="0" ArrayMatch="true" />
            <Feature Name="Angle" Mapping="FlawAngle" Value="0" ArrayMatch="true" />
