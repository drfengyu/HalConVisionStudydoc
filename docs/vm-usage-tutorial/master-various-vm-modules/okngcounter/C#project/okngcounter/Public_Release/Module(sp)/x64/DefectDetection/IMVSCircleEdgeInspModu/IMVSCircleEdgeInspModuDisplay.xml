        <Object Name="StandardCircleResult" Type="circle" okcolor="#66ff00" ngcolor="#ff0000" hotcolor="#66ff00" opacity="1" linetype="1" linewidth="1" IsDisplay="true">
          <Features>
            <Feature Name="CenterX" Mapping="StandardCircleCenterX" Value="0" />
            <Feature Name="CenterY" Mapping="StandardCircleCenterY" Value="0" />
            <Feature Name="MajorRadius" Mapping="StandardCircleRadius" Value="0" />
            <Feature Name="MinorRadius" Mapping="StandardCircleRadius" Value="0" />
            <Feature Name="Tips" Value="CircleSite:Center({0},{1}) Radius={2}" Mapping="StandardCircleCenterX,StandardCircleCenterY,StandardCircleRadius" />
